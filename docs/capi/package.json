    "ts-morph": "^6.0.2",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/clone-deep": "^4.0.1",
    "@types/fs-extra": "^8.0.1",
